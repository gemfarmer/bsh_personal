html
	margin: 0
	padding: 0
	body
		background-image:url('/images/background-texture.jpg');
		margin: 0
		box-sizing: border-box;
		font: "Lucida Grande", Helvetica, Arial, sans-serif;
		color: #989898;
		margin-left: 0
		margin-right: 0
		padding: 0
		.wrapper

			margin-left: 0
			margin-right: 0
			text-align:center
			height: 100px
			padding: 0
			.header
				width: 100%
				// background-color: white
				height: 100px
				font-size: 40px

				padding-top: 30px
				padding-left: 0

			div.tabbable
				padding: 0

				ul.nav-tabs
					li
						a:link
							color: #989898
					li.active
						a
							background-color: #989898
							color: white

			#self

				padding: 30px
				padding-bottom: 0px
				
				border-radius: 50%
			.text-field
				padding-top: 30px
				text-align: left
				padding-right: 30px
				padding-left: 30px
				// padding-left: 10px
				// padding-right: 10px
				background-color: #989898
				height: 600px
				color: white
				width: 100
				border-bottom-right-radius: 5px
				border-bottom-left-radius: 5px
				
				

h3.
.form-group
	margin-left: 10px
	font-size: 20px
.links 
	font-size: 20px


			

				


@keyframes mymove{
0%   {
        top:0px;
        background-color: darkblue;
}
25%  {
        top:422px;
        background-color: steelblue;
}
50%  {
        top:105px;
        /*background-color: blue;*/
        }
75%  {
        top:317px;
        background-color: skyblue;
}
100% {
        top:0px;
        background-color: lightblue;
}
}
@-webkit-keyframes mymove{
0%   {
        top:0px;
        background-color: darkblue;
}
25%  {
        top:422px;
        background-color: steelblue;
}
50%  {
        top:105px;
        /*background-color: blue;*/
        }
75%  {
        top:317px;
        background-color: skyblue;
}
100% {
        top:0px;
        background-color: lightblue;
}
}


		