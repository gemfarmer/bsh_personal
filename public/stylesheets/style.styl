@keyframes mymove{
0%   {
        top:0px;
        background-color: darkblue;
}
25%  {
        top:422px;
        background-color: steelblue;
}
50%  {
        top:105px;
        /*background-color: blue;*/
        }
75%  {
        top:317px;
        background-color: skyblue;
}
100% {
        top:0px;
        background-color: lightblue;
}
}
@-webkit-keyframes mymove{
0%   {
        top:0px;
        background-color: darkblue;
}
25%  {
        top:422px;
        background-color: steelblue;
}
50%  {
        top:105px;
        /*background-color: blue;*/
        }
75%  {
        top:317px;
        background-color: skyblue;
}
100% {
        top:0px;
        background-color: lightblue;
}
}


		